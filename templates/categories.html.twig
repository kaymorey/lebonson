{% extends 'layout/base.html.twig' %}
{% block title %}{{ category.title }}{% endblock %}
{% block content %}
<div class="container-fluid">
	<div class="row-fluid">
		<div class="span3">
			{% include 'content/search.html.twig' with {'mainArtists' : mainArtists} %}
		</div>
		<div class="span9">
			<div class="row-fluid">
			{% for product in products %}
			{% include 'list/products.html.twig' with {'products' : products} %}
			{% endfor %}
			</div>
		</div>
	</div>
</div>
{% endblock %}